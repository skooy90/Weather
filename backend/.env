# 환경 변수는 Render 대시보드에서 설정합니다.

# 서버 설정
PORT=3000
NODE_ENV=production

# 데이터베이스 설정
# MongoDB URI는 Render 대시보드에서 환경변수로 설정해야 합니다
MONGODB_URI=your_mongodb_uri

# CORS 설정
CORS_ORIGIN=https://weather-of7u.onrender.com

# 보안 설정
# JWT_SECRET은 Render 대시보드에서 환경변수로 설정해야 합니다
JWT_SECRET=your_jwt_secret

# 로깅 설정
LOG_LEVEL=info
LOG_FILE=logs/app.log 